pipeline{
    agent any
    environment
    {
        name1="jeff"
        name2="jail1"
        name3="jais"
    }
    stages{
        stage("build"){
        environment{
            name4="jais"
        }
        steps{
            echo "name ${name1}"
            echo "name ${name2}"
            echo "name ${name3}"
        }
        }
        stage ("check variabke")
        {
            steps{

               echo "name ${name1}"
            echo "name ${name2}"
            echo "name ${name3}" 
            }
        }
    
    }
}